# MostFrequent

import operator

def most_frequent(test_string):
    
    string_dict = dict()
    for letter in test_string:
        if letter not in string_dict:
            string_dict[letter] = test_string.count(letter)
            
    ordered_answer = sorted(string_dict.items(), key=operator.itemgetter(1), reverse=True)
    return ordered_answer


test_string=input("Please enter any string : ")
print(most_frequent(test_string))
